{
  "name": "homeschool",
  "version": "1.0.0",
  "private": true,
  "description": "家校互联",
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "cross-env CODE_ENV=dev vue-cli-service serve",
    "build": "cross-env CODE_ENV=production vue-cli-service build",
    "format-code": "prettier --write \"src/**/*.js\" \"src/**/*.vue\""
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.3.2",
    "dayjs": "^1.11.4",
    "vant": "^2.11.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.11",
    "@vue/cli-plugin-eslint": "4.5.11",
    "@vue/cli-plugin-router": "4.5.11",
    "@vue/cli-plugin-vuex": "4.5.11",
    "@vue/cli-service": "4.5.11",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^2.5.1",
    "vue-template-compiler": "^2.6.10"
  },
  "appid": "homeschool",
  "packageName": "homeschool",
  "routerMode": "history",
  "serverName": "homeschool",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
